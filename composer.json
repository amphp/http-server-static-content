{
    "name": "amphp/http-server-static-content",
    "homepage": "https://github.com/amphp/http-server-static-content",
    "description": "Static content request handler for Amp's HTTP server.",
    "keywords": [
        "http",
        "server",
        "file",
        "static"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Lowrey",
            "email": "rdlowrey@php.net"
        },
        {
            "name": "Bob Weinand"
        },
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        },
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "amphp/amp": "^3",
        "amphp/byte-stream": "^2",
        "amphp/cache": "^2",
        "amphp/file": "^3",
        "amphp/http-server": "^3",
        "amphp/http": "^2-dev",
        "amphp/pipeline": "^1",
        "cash/lrucache": "^1",
        "revolt/event-loop": "^1 || ^0.2.4"
    },
    "require-dev": {
        "amphp/http-server-router": "^2",
        "amphp/log": "^2",
        "amphp/phpunit-util": "^3",
        "amphp/php-cs-fixer-config": "^2",
        "phpunit/phpunit": "^9",
        "danielmiessler/sec-lists": "2022.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Amp\\Http\\Server\\StaticContent\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Http\\Server\\StaticContent\\Test\\": "test"
        }
    },
    "scripts": {
        "test": "@php -dzend.assertions=1 -dassert.exception=1 ./vendor/bin/phpunit",
        "code-style": "@php ./vendor/bin/php-cs-fixer fix"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "danielmiessler/sec-lists",
                "version": "2022.1",
                "source": {
                    "url": "https://github.com/danielmiessler/SecLists",
                    "type": "git",
                    "reference": "2022.1"
                }
            }
        }
    ]
}
